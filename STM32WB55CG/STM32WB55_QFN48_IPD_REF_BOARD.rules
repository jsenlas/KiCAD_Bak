
(rules PCB STM32WB55_QFN48_IPD_REF_BOARD
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 42)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.5)
    )
    (layer_rule GND
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.5)
    )
    (layer_rule PWR
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.5)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.4)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-3]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle GND 800.0 0.0 0.0)
    )
    (shape
      (circle PWR 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-3]_800:400_um" "Via[0-3]_800:400_um" default
  )
  (via 
    "Via[0-3]_800:400_um-kicad_default" "Via[0-3]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-3]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-3]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C17-Pad1)" NRST +3V3 +1V5 "Net-(R_1-Pad2)" GND "Net-(L1-Pad2)" "I2C_SCL"
    "I2C_SDA" "Net-(FB3-Pad1)" "BNO055_INT" "Net-(U4-Pad13)" "Net-(U4-Pad12)" "Net-(C24-Pad1)" "Net-(U4-Pad8)" "Net-(U4-Pad7)"
    "Net-(U4-Pad21)" "Net-(U4-Pad22)" "Net-(U4-Pad23)" "Net-(U4-Pad24)" "Net-(C25-Pad1)" "Net-(U4-Pad1)" RF1 "SPI_MOSI"
    "SPI_MISO" "LED_BLUE" "SPI_SCK" "Net-(C14-Pad1)" "Net-(J1-Pad4)" +5V "Net-(FL1-Pad1)" "Net-(C19-Pad1)"
    "BNO055_RESET" 3v3fromUSB "Net-(IC2-Pad6)" "Net-(IC2-Pad4)" "USB_D+" "USB_D-" "Net-(C13-Pad1)" 3v3fromBAT
    "ADC_ALERT" "GENERAL_PIN_OUT3" "GENERAL_PIN_OUT2" "SYS_JTDO-SWO" "Net-(U2-Pad42)" "SYS_JTCK-SWCLK" "SYS_JTMS-SWDIO" "Net-(U2-Pad36)"
    "Net-(L3-Pad2)" "Net-(U2-Pad29)" "Net-(U2-Pad28)" "Net-(U2-Pad27)" "Net-(U2-Pad26)" "Net-(U2-Pad25)" "Net-(U2-Pad24)" "Net-(U2-Pad19)"
    "Net-(U2-Pad18)" "Net-(U2-Pad17)" "GENERAL_PIN_OUT1" "Net-(U2-Pad12)" "Net-(U2-Pad11)" "Net-(U2-Pad9)" "Net-(R4-Pad1)" "Net-(C18-Pad1)"
    "Net-(C23-Pad1)" "Net-(R10-Pad1)" "Net-(R11-Pad1)" "Net-(C22-Pad2)" "Net-(U5-Pad7)" "Net-(R13-Pad1)" "Net-(D1-Pad2)" "Net-(J4-Pad4)"
    "Net-(J4-Pad3)" "Net-(J4-Pad2)" "Net-(L2-Pad2)" "Net-(C5-Pad1)" "Net-(S1-PadMP4)" "Net-(S1-PadMP3)" "Net-(S1-PadMP2)" "Net-(S1-PadMP1)"
    "Net-(S1-Pad3)" "Net-(S2-PadMP4)" "Net-(S2-PadMP3)" "Net-(S2-PadMP2)" "Net-(S2-PadMP1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)